{
    "name": "ryan-rushton.github.io",
    "version": "1.0.0",
    "private": true,
    "homepage": "https://ryan-rushton.github.io",
    "dependencies": {
        "next": "12.0.7",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "react-icons": "4.3.1",
        "react-particles-js": "3.6.0",
        "sass": "1.45.2"
    },
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "export": "next export",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
        "type-check": "tsc --noEmit",
        "start": "next start",
        "predeploy": "yarn build && yarn export",
        "deploy": "gh-pages -b master -d out"
    },
    "devDependencies": {
        "@types/node": "16.11.17",
        "@types/react": "17.0.38",
        "@types/react-dom": "17.0.11",
        "@typescript-eslint/eslint-plugin": "5.8.1",
        "@typescript-eslint/parser": "5.8.1",
        "eslint": "8.5.0",
        "eslint-config-next": "12.0.7",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.28.0",
        "eslint-plugin-react-hooks": "4.3.0",
        "gh-pages": "3.2.3",
        "prettier": "2.5.1",
        "prettier-plugin-organize-imports": "2.3.4",
        "tsparticles": "1.37.5",
        "typescript": "4.5.4"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
